#!/bin/bash

dir_resolve() {
  local dir=`dirname "$1"`
  local file=`basename "$1"`
  pushd "$dir" &>/dev/null || return $? # On error, return error code
  echo "`pwd -P`/$file" # output full, link-resolved path with filename
  popd &> /dev/null
}
base=`realpath "$0"`
base2=`dirname "$base"`
base1=`dirname "$base2"`
n3="/main.py"
n4=$base1$n3
run=`realpath "$n4"`


function argv {
    for a in ${BASH_ARGV[*]} ; do
      echo -n "$a "
    done
    echo
}

python $run `argv`